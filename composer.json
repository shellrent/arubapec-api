{
    "name": "shellrent/arubapec-api",
    "description": "Client API PHP library for interacting with the ArubaPEC API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shellrent",
            "email": "sviluppo@shellrent.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.9",
        "nesbot/carbon": "^2.72"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Shellrent\\Arubapec\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shellrent\\Arubapec\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": "php-cs-fixer fix --config=.php-cs-fixer.dist.php --dry-run --diff",
        "lint:fix": "php-cs-fixer fix --config=.php-cs-fixer.dist.php"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
